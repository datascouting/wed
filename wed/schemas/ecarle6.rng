                  </oneOrMore>
            
                  <optional>
                     <choice>
                
                
                        <oneOrMore>
                           <group>
                    
                              <ref name="ecarle6model.divLike"/>
                    
                    
                              <zeroOrMore>
                                 <choice>
                                    <ref name="ecarle6model.global"/>
                                    <ref name="ecarle6model.divGenLike"/>
