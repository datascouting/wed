               <ref name="ecarle6model.gLike"/>
               <ref name="ecarle6sense"/>
               <ref name="ecarle6model.entryPart.top"/>
               <ref name="ecarle6model.phrase"/>
               <ref name="ecarle6model.global"/>
            </choice>
         </zeroOrMore>
         <ref name="ecarle6att.global.attributes"/>
         <ref name="ecarle6att.lexicographic.attributes"/>
         <ref name="ecarle6att.typed.attributes"/>
