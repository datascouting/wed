               <text/>
               <ref name="ecarle6model.gLike"/>
               <ref name="ecarle6model.global"/>
               <ref name="ecarle6model.inter"/>
               <ref name="ecarle6model.phrase"/>
               <ref name="ecarle6def"/>
               <ref name="ecarle6etym"/>
               <ref name="ecarle6gramGrp"/>
               <ref name="ecarle6lbl"/>
               <ref name="ecarle6usg"/>
               <ref name="ecarle6xr"/>
            </choice>
