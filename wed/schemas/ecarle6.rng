         <ref name="ecarle6att.global.attributes"/>
         <ref name="ecarle6att.typed.attribute.subtype"/>
         <ref name="ecarle6att.lexicographic.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">classifies the label using any convenient typology.</a:documentation>
